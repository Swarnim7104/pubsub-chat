{
    "network": {
        "host": "localhost",
        "port": 5555,
        "protocol": "tcp"
    },
    "logging": {
        "level": "INFO",
        "file": "pubsub.log",
        "max_file_size": "10MB",
        "backup_count": 3
    },
    "channels": {
        "default_channels": ["news", "tech", "general", "alerts"],
        "max_message_length": 1000,
        "rate_limit": {
            "messages_per_minute": 60,
            "enabled": true
        }
    },
    "features": {
        "message_persistence": true,
        "timestamps": true,
        "user_validation": true
    }
}